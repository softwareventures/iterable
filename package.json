{
  "name": "@softwareventures/iterable",
  "version": "0.0.0-development",
  "description": "Pure functional Iterable traversal",
  "keywords": [
    "Iterable",
    "TypeScript",
    "functional",
    "lambda"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/iterable",
  "bugs": "https://github.com/softwareventures/iterable/issues",
  "repository": "github:softwareventures/iterable",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "@softwareventures/nullable": "^1.0.0 || ^1.2.1 || ^2.0.0",
    "@softwareventures/ordered": "^0.2.2 || ^1.0.0",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "4.3.2",
    "@softwareventures/prettier-config": "3.0.0",
    "@softwareventures/semantic-release-config": "2.0.2",
    "@softwareventures/tsconfig": "5.1.1",
    "ava": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "prettier": "2.6.2",
    "semantic-release": "17.4.7",
    "ts-expect": "1.3.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "publishConfig": {
    "access": "public"
  }
}
